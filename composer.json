{
  "name": "underwear/laravel-myteam-logging",
  "description": "Send logs to MyTeam chat via MyTeam bot",
  "license": "MIT",
  "autoload": {
    "psr-4" : {
      "MyteamLogger\\" : "src"
    }
  },
  "require": {
    "php": "^7.1.3",
    "monolog/monolog": "^1.23|^2.0",
    "laravel/framework": "5.6.*|5.7.*|5.8.*|^6.0|^7.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MyteamLogger\\MyteamLoggerServiceProvider"
      ]
    }
  }
}
